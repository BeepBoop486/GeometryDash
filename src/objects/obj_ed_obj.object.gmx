        <arguments>
          <argument>
            <kind>1</kind>
            <string>&#13;
	if global.selected_mode == 0 {&#13;
		if global.intab == 0 {&#13;
			global.toput = obj_solid&#13;
		} if global.intab == 1 {&#13;
			global.toput = obj_spike&#13;
		}&#13;
	} if global.selected_mode == 1 &amp;&amp; global.selected_obj != 0 {&#13;
		if pos == 1 {&#13;
			with(global.selected_obj) {&#13;
				x -= 61&#13;
			}&#13;
		} if pos == 2 {&#13;
			with(global.selected_obj)&#13;
			{&#13;
				x += 61&#13;
			}&#13;
		}&#13;
	}</string>
