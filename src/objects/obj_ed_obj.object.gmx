			&#13;
			if obj_spr_w == 61 &amp;&amp; obj_spr_h == 61 {&#13;
				scale = .5&#13;
			}&#13;
		}&#13;
		&#13;
		draw_sprite_ext(obj_spr, 0, x, y, scale, scale, 0, c_white, 100)&#13;
	} else if global.selected_mode == 1 //if we're in edit mode&#13;
